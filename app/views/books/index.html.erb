<h1>Listing books</h1>

<table>
  <tr>
    <th> title</th>
    <th>Description</th>
    <th>Author</th>
    <th>Publisher</th>
    <th>Published date</th>
    <th>Price</th>
    <th>Img url</th>
    <th>Total rating value</th>
    <th>Rating count</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @books.each do |book| %>
  <tr>
    <td><%= book. title %></td>
    <td><%= book.description %></td>
    <td><%= book.author %></td>
    <td><%= book.publisher %></td>
    <td><%= book.published_date %></td>
    <td><%= book.price %></td>
    <td><%= book.img_url %></td>
    <td><%= book.total_rating_value %></td>
    <td><%= book.rating_count %></td>
    <td><%= link_to 'Show', book %></td>
    <td><%= link_to 'Edit', edit_book_path(book) %></td>
    <td><%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Book', new_book_path %>
