<!DOCTYPE html>
<html>
<head>
  <title>MyAssignment</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body id="<%= controller.controller_name %>">
	<div class="row">
		<div class="col-md-3">
			<div class="sidebar-nav">
				<div class="well" style="width:300px; padding: 8px 0;">
					<ul class="nav nav-list"> 
		  				<li class="nav-header">Admin Menu</li>        
		  				<li><a href="index"><i class="glyphicon glyphicon-home"></i> Dashboard</a></li>
          				<li><a href="#"><i class="glyphicon glyphicon-envelope"></i> Messages <span class="badge badge-info">4</span></a></li>
          				<li><a href="#"><i class="glyphicon glyphicon-comment"></i> Comments <span class="badge badge-info">10</span></a></li>
		  				<li class="active"><a href="#"><i class="glyphicon glyphicon-user"></i> Members</a></li>
          				<li class="divider"></li>
		  				<li><a href="#"><i class="glyphicon glyphicon-comment"></i> Settings</a></li>
		  				<li><a href="#"><i class="glyphicon glyphicon-share"></i> Logout</a></li>
					</ul>
					<div id="cart">
						<%= render(@cart) %>
					</div>
				</div>
			</div>
		</div>
		<div class="col-md-9" id="main-content">
			<%= yield %>
		</div>
	</div>
</body>
</html>
